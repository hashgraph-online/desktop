{
  "name": "hol-desktop",
  "productName": "HOL Desktop",
  "version": "0.2.0",
  "description": "Desktop application for HashgraphOnline",
  "main": ".vite/build/main.js",
  "type": "module",
  "config": {
    "forge": "./forge.config.mjs"
  },
  "scripts": {
    "start": "npx electron-forge start",
    "package": "npm run prepare-icons && electron-forge package",
    "make": "npm run prepare-icons && electron-forge make",
    "publish": "npm run prepare-icons && electron-forge publish",
    "setup": "node scripts/setup-legal-files.js",
    "dev": "npm run setup && npm run electron-rebuild && npx electron-forge start",
    "dev:log": "node scripts/dev-with-logging.js",
    "build": "tsc && npm run prepare-icons && electron-forge package",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test:coverage": "jest --coverage",
    "prepare-icons": "node scripts/prepare-icons.js",
    "make:all": "npx tsx scripts/build-all-platforms.ts",
    "dist": "npm run build",
    "electron-rebuild": "npx electron-rebuild -f -w better-sqlite3",
    "postinstall": "npx electron-rebuild -f -w better-sqlite3",
    "dist:mac": "npm run prepare-icons && electron-forge make --platform darwin",
    "dist:win": "npm run prepare-icons && electron-forge make --platform win32",
    "dist:linux": "npm run prepare-icons && electron-forge make --platform linux",
    "playwright:example": "npx tsx scripts/playwright-automation-example.ts",
    "playwright:install": "npx playwright install",
    "mcp:sort-check": "npx tsx scripts/mcp-sort-check.ts",
    "mcp:verify": "npx tsx scripts/mcp-verify.ts"
  },
  "keywords": [
    "electron",
    "vite",
    "react",
    "hedera",
    "ai-agent"
  ],
  "author": "Hashgraph Online",
  "license": "MIT",
  "packageManager": "pnpm@10.14.0+sha512.ad27a79641b49c3e481a16a805baa71817a04bbe06a38d17e60e2eaee83f6a146c6a688125f5792e48dd5ba30e7da52a5cda4c3992b9ccf333f9ce223af84748",
